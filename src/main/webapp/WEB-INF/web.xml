<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ImageController</servlet-name>
  	<display-name>ImageController</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.imageManagementUtility.controller.ImageController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PhotoController</servlet-name>
  	<display-name>PhotoController</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.imageManagementUtility.controller.PhotoController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ImageDataController</servlet-name>
  	<display-name>ImageDataController</display-name>
  	<description></description>
  	<servlet-class>com.nagarro.imageManagementUtility.controller.ImageDataController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ImageController</servlet-name>
  	<url-pattern>/ImageController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PhotoController</servlet-name>
  	<url-pattern>/PhotoController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ImageDataController</servlet-name>
  	<url-pattern>/ImageDataController</url-pattern>
  </servlet-mapping>
</web-app>
